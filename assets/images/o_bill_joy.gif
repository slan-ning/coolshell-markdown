        function fixedContainer(element, left) {
            element = $(element);
            var top = element.height() - $(window).height() + $("#csdn-toolbar").height() + 20;
            if ($(window).scrollTop() > top && top > 0) {
                element.css({
                    'top': 'auto',
                    'bottom': 0,
                    'left': $("#main").offset().left + left - $('html').scrollLeft()
                });
                !element.hasClass('fixed') ? element.addClass("fixed") : ""
            } else if (top < 0 && $(window).scrollTop() > $("#csdn-toolbar").height()) {
                element.css({
                    'top':  8,
                    "bottom": 'auto',
                    'left': $("#main").offset().left + left - $('html').scrollLeft()
                })
                !element.hasClass('fixed') ? element.addClass("fixed") : ""
            } else {
                element.removeClass("fixed");
            }
            if ($(window).scrollTop() < 10) {
                element.removeClass("fixed")
            }
        }

