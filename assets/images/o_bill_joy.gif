                '</div>'+
                '<span class="user_name">'+nickName+'</span>'+
                '<span class="read_num">阅读数：'+data.views+'</span>'+
                '</div>'+
                '</a>'+
                '</div>';
        }

        //鲲鹏广告调用
        function getLeftAdLocation(id) {
            $.ajax({
                url: 'https://kunpeng.csdn.net/ad/list?positions='+id,
